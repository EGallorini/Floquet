@article{marques-lopez-blackburn-2004,
  title = {Bifurcations in Systems with {{Z2}} Spatio-Temporal and {{O}}(2) Spatial Symmetry},
  author = {Marques, F. and Lopez, J. M. and Blackburn, H. M.},
