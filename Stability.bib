@article{leweke-williamson-1998,
  title = {Three-Dimensional Instabilities in Wake Transition},
  author = {Leweke, T. and Williamson, C. H. K.},
  year = {1998},
  month = jul,
  journal = {European Journal of Mechanics - B/Fluids},
