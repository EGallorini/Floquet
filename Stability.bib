@article{prasanth-mittal-2008,
  title = {Vortex-Induced Vibrations of a Circular Cylinder at Low {{Reynolds}} Numbers},
  author = {Prasanth, T. K. and Mittal, S.},
  year = {2008},
  month = jan,
  journal = {J. Fluid Mech.},
  volume = {594},
  pages = {463--491},
