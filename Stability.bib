@article{citro-etal-2017,
  title = {Efficient Stabilization and Acceleration of Numerical Simulation of Fluid Flows by Residual Recombination},
  author = {Citro, V. and Luchini, P. and Giannetti, F. and Auteri, F.},
  year = {2017},
  month = sep,
  journal = {J. Comp. Phys.},
